      "evaluates": {
        "@id": "_:intervention-phonics",
        "@type": "Intervention",
        "implementationName": {
          "@type": "StringCodingAnnotation",
          "codedValue": "Structured phonics programme",
          "codedBy": "EEF",
          "status": "coded"
        },
        "educationTheme": [
          {
            "@type": "EducationThemeCodingAnnotation",
            "codedValue": { "@id": "esea:C00074" },
            "codedBy": "EEF",
            "status": "coded"
          }
        ],
        "duration": {
          "@type": "NumericCodingAnnotation",
          "codedValue": 24,
          "supportingText": "24 weeks",
          "codedBy": "EEF",
          "status": "coded"
        }
      },
      "comparedTo": {
        "@id": "_:control-bau",
        "@type": "ControlCondition"
      }
    },

    {
      "_comment": "Finding 2: Pre-post design — both conditions are temporal",
      "@id": "_:finding-prepost",
      "@type": "Finding",
      "evaluates": {
        "@id": "_:post-test",
