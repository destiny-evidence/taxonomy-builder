# #############################################################################
# FIELD 4: Outcome
#
# Property definition:
#   identifier:    outcome
#   label:         Outcome
#   description:   The type of educational or other outcome measured in a finding.
#   domain:        evrepo:Outcome
#   range:         esea:OutcomeCodingAnnotation
#   valueScheme:   esea:OutcomeScheme
#   allowMultiple: true
#   hierarchy:     2-level (top-level categories → specific outcomes)
# #############################################################################

# --- Typed Concept Class ---

esea:OutcomeConcept a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Outcome Concept"@en .

# --- CodingAnnotation Subclass ---

esea:OutcomeCodingAnnotation a owl:Class ;
    rdfs:subClassOf evrepo:CodingAnnotation ;
    rdfs:label "Outcome Coding Annotation"@en ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty evrepo:codedValue ;
        owl:allValuesFrom esea:OutcomeConcept
    ] .

# --- Domain Property ---

esea:outcome a owl:ObjectProperty ;
    rdfs:label "outcome"@en ;
    rdfs:comment "Type of educational or other outcome measured. 2-level hierarchy: access/education/externalities/learning/returns categories with specific outcome types. Multiple values may be coded."@en ;
    rdfs:domain evrepo:Outcome ;
    rdfs:range esea:OutcomeCodingAnnotation .

# --- Concept Scheme ---

esea:OutcomeScheme a skos:ConceptScheme ;
    rdfs:label "Outcome Scheme"@en ;
    rdfs:comment "Types of educational and related outcomes measured in studies. 2-level hierarchy with 5 top-level categories."@en ;
    skos:hasTopConcept esea:C00096 , esea:C00105 , esea:C00114 , esea:C00122 , esea:C00130 .

