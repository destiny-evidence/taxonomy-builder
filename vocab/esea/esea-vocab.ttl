@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix evrepo: <https://evrepo.example.org/vocab/> .
@prefix esea: <https://esea.example.org/vocab/> .

# =============================================================================
# ESEA Extension Ontology
# =============================================================================
#
# Education Sector Evidence Architecture vocabulary. Extends DESTINY Core
# with education-specific classes, properties, and concept schemes.
#
# Concept URIs use numeric identifiers for stability. Use skos:prefLabel for
# display and skos:notation for short codes.
#
# Each concept scheme has a corresponding OWL class (subclass of skos:Concept)
# for semantic typing and range constraints.
#
# =============================================================================

esea: a owl:Ontology ;
    rdfs:label "ESEA Vocabulary" ;
    rdfs:comment """Education Sector Evidence Architecture vocabulary for 
    coding education research. Extends DESTINY Core with education-specific 
    properties and controlled vocabularies.""" ;
    owl:imports evrepo: ;
    dcterms:created "2025-01-01"^^xsd:date ;
    owl:versionInfo "0.1.0" .


# =============================================================================
# CONCEPT CLASSES (subclasses of skos:Concept)
# =============================================================================

esea:EducationLevel a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Education Level" ;
    rdfs:comment "A concept representing an education level (ISCED-aligned)." .

esea:EducationSetting a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Education Setting" ;
    rdfs:comment "A concept representing an educational institution or delivery context." .

esea:EducationTheme a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Education Theme" ;
    rdfs:comment "A concept representing a strategic domain of focus within education." .

esea:EducationOutcome a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Education Outcome" ;
    rdfs:comment "A concept representing a type of educational outcome." .

esea:StudyDesign a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Study Design" ;
    rdfs:comment "A concept representing a research methodology." .

esea:ImplementerType a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Implementer Type" ;
    rdfs:comment "A concept representing a category of implementing organisation." .

esea:ImplementationComponent a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Implementation Component" ;
    rdfs:comment "A concept representing an element of intervention delivery." .

esea:SampleFeature a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Sample Feature" ;
    rdfs:comment "A concept representing a demographic or equity characteristic." .

esea:ParticipantType a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Participant Type" ;
    rdfs:comment "A concept representing a type of research participant." .

esea:FidelityType a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Fidelity Type" ;
    rdfs:comment "A concept representing a type of implementation fidelity information." .

esea:EffectSizeType a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Effect Size Type" ;
    rdfs:comment "A concept representing a type of effect size metric." .


# =============================================================================
# OBJECT PROPERTIES
# =============================================================================

esea:educationLevel a owl:ObjectProperty ;
    rdfs:label "education level" ;
    rdfs:comment "The ISCED education level of the target population." ;
    # Fix: domain is Context, not Finding â€” educationLevel describes the setting,
    # not the comparison. Matches briefing field reference table and JSON-LD examples.
    rdfs:domain evrepo:Context ;
    rdfs:range esea:EducationLevel .

esea:educationSetting a owl:ObjectProperty ;
    rdfs:label "education setting" ;
    rdfs:comment "The type of educational institution or delivery context." ;
    rdfs:domain evrepo:Context ;
    rdfs:range esea:EducationSetting .

esea:educationTheme a owl:ObjectProperty ;
    rdfs:label "education theme" ;
    rdfs:comment "Strategic domain of focus within education systems." ;
    rdfs:domain evrepo:Intervention ;
    rdfs:range esea:EducationTheme .

esea:educationOutcome a owl:ObjectProperty ;
    rdfs:label "education outcome" ;
    rdfs:comment "Education-specific outcome type classification." ;
    rdfs:domain evrepo:Outcome ;
    rdfs:range esea:EducationOutcome .

esea:studyDesign a owl:ObjectProperty ;
    rdfs:label "study design" ;
    rdfs:comment "Research methodology used in the investigation." ;
    rdfs:domain evrepo:Investigation ;
    rdfs:range esea:StudyDesign .

esea:implementerType a owl:ObjectProperty ;
    rdfs:label "implementer type" ;
    rdfs:comment "Category of implementing organisation." ;
    rdfs:domain evrepo:Intervention ;
    rdfs:range esea:ImplementerType .

esea:implementationComponent a owl:ObjectProperty ;
    rdfs:label "implementation component" ;
    rdfs:comment "Elements included in delivery (training, materials, etc.). Multiple values allowed." ;
    rdfs:domain evrepo:Intervention ;
    rdfs:range esea:ImplementationComponent .

esea:implementationFidelity a owl:ObjectProperty ;
    rdfs:label "implementation fidelity" ;
    rdfs:comment "Type of fidelity information provided." ;
    rdfs:domain evrepo:Intervention ;
    rdfs:range esea:FidelityType .

esea:sampleFeature a owl:ObjectProperty ;
    rdfs:label "sample feature" ;
    rdfs:comment "Key demographic or equity characteristic of the sample. Multiple values allowed." ;
    rdfs:domain evrepo:Finding ;
    rdfs:range esea:SampleFeature .

esea:participantType a owl:ObjectProperty ;
    rdfs:label "participant type" ;
    rdfs:comment "Type of participants (students, teachers, etc.)." ;
    rdfs:domain evrepo:Finding ;
    rdfs:range esea:ParticipantType .


# =============================================================================
# DATATYPE PROPERTIES
# =============================================================================

esea:hasBaselineDifferences a owl:DatatypeProperty ;
    rdfs:label "has baseline differences" ;
    rdfs:comment """Whether the study reports group differences at baseline.
    Relevant for RCTs and QEDs where group comparisons are made.""" ;
    rdfs:domain evrepo:Investigation ;
    rdfs:range xsd:boolean .

esea:hasCostData a owl:DatatypeProperty ;
    rdfs:label "has cost data" ;
    rdfs:comment "Whether the study contains quantitative costing information." ;
    rdfs:domain evrepo:Investigation ;
    rdfs:range xsd:boolean .


